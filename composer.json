{
    "name": "gazugafan/laravel-temporal",
    "description": "Temporal models and versioning for Laravel 5",
    "keywords": [
        "laravel",
        "temporal",
        "model",
        "trait"
    ],
    "homepage": "https://github.com/gazugafan/laravel-temporal",
    "license": "MIT",
    "require": {
        "php" : ">=8.0.2",
        "illuminate/database": "^9.0",
        "illuminate/events": "^9.0",
        "doctrine/dbal": "^2.5",
        "laravel/helpers": "^1.2"
    },
    "require-dev": {
		"phpunit/phpunit" : "^7.5.15|^8.4|^9.0",
		"mockery/mockery": "^0.9.4",
		"php-coveralls/php-coveralls": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Gazugafan\\Temporal\\": "src"
        }
    },
	"autoload-dev": {
		"psr-4": {
			"Gazugafan\\Temporal\\Test\\": "tests"
		}
	},
    "scripts": {
    }
}
